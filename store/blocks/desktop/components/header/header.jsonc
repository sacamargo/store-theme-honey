{
    "sticky-layout#header-desktop": {
        "props": {
            "blockClass": "sticky-header"
        },
        "children": [
            "flex-layout.row#1-desktop",
            "flex-layout.row#mega-menu",
            "header-row#2-desktop"
        ]
    },
    "flex-layout.row#1-desktop": {
        "props": {
            "blockClass": "header-row-1",
            "colSizing": "auto"
        },
        "children": [
            "flex-layout.row#global__drawer--and--logo",
            "search-bar",
            "flex-layout.row#header__link--container",
            "flex-layout.row#global__minicart--and--login"
        ]
    },
    "flex-layout.row#global__minicart--and--login": {
        "children": [
            "minicart.v2",
            "login"
        ],
        "props": {
            "blockClass": "minicart-login--container",
            "colSizing": "auto"
        }
    },
    "flex-layout.row#global__drawer--and--logo": {
        "children": [
            "flex-layout.col#trigger-mega-menu",
            "logo"
        ],
        "props": {
            "blockClass": "drawer-logo--container"
        }
    },
    "logo": {
        "props": {
            "width": 32,
            "height": 20,
            "mobileWidth": 32,
            "mobileHeight": 16,
            "url": "/arquivos/itg--logo.svg"
        }
    },
    "header-row#2-desktop": {
        "props": {
            "blockClass": "menu-home-2"
        },
        "children": [
            "vtex.menu@2.x:menu#websites"
        ]
    },
